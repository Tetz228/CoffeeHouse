<Window x:Class="WpfApp1.Admin.Add.AddShiftsDatesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1.Admin.Add"
        mc:Ignorable="d"
        Title="Добавить дату" Height="163.844" Width="273.562" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <DatePicker x:Name="DatePickerDate" IsTodayHighlighted="True" HorizontalAlignment="Left" Margin="71,39,0,0" VerticalAlignment="Top" FontFamily="Verdana" FontSize="14" Width="125"/>
        <Button Style="{StaticResource button}" x:Name="ButtonAdd" Click="ButtonAdd_Click" Content="Добавить" Margin="10,0,0,10" VerticalAlignment="Bottom" Width="81"/>
        <Button Style="{StaticResource button}" x:Name="Cancel" Click="Cancel_Click" Content="Отмена" Margin="0,0,10,10" HorizontalAlignment="Right" VerticalAlignment="Bottom" Width="86"/>
    </Grid>
</Window>
